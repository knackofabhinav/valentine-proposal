         const containerRect = container.getBoundingClientRect();\n   532â†’            const btnRect = noBtn.getBoundingClientRect();\n   533â†’\n   534â†’            const maxX = containerRect.width - btnRect.width;\n   535â†’            const maxY = containerRect.height - btnRect.height;\n   536â†’\n   537â†’            const randomX = Math.random() * maxX;\n   538â†’            const randomY = Math.random() * maxY;\n   539â†’\n   540â†’            noBtn.style.left = randomX + 'px';\n   541â†’            noBtn.style.top = randomY + 'px';\n   542â†’\n   543â†’            // Make button smaller each time\n   544â†’            if (noMoveCount > 3) {\n   545â†’                noBtn.style.transform = `scale(${Math.max(0.5, 1 - noMoveCount * 0.1)})`;\n   546â†’            }\n   547â†’\n   548â†’            // Change text after a few attempts\n   549â†’            if (noMoveCount === 3) {\n   550â†’                noBtn.textContent = 'Are you sure?';\n   551â†’            } else if (noMoveCount === 5) {\n   552â†’                noBtn.textContent = 'Really?';\n   553â†’            } else if (noMoveCount >= 7) {\n   554â†’                noBtn.textContent = 'Just say yes!';\n   555â†’            }\n   556â†’        }\n   557â†’\n   558â†’        // Handle Yes click\n   559â†’        function handleYes() {\n   560â†’            document.getElementById('proposal').classList.remove('show');\n   561â†’            document.getElementById('success').classList.add('show');\n   562â†’            createConfetti();\n   563â†’\n   564â†’            // Update love text with name\n   565â†’            const params = getUrlParams();\n   566â†’            if (params.name) {\n   567â†’                const name = decodeURIComponent(params.name);\n   568â†’                document.getElementById('loveText').textContent = `Thank you, ${name}! You've made me so happy!`;\n   569â†’            }\n   570â†’        }\n   571â†’\n   572â†’        // Confetti effect\n   573â†’        function createConfetti() {\n   574â†’            const canvas = document.getElementById('confettiCanvas');\n   575â†’            const ctx = canvas.getContext('2d');\n   576â†’\n   577â†’            canvas.width = window.innerWidth;\n   578â†’            canvas.height = window.innerHeight;\n   579â†’\n   580â†’            const confettiCount = 200;\n   581â†’            const confetti = [];\n   582â†’\n   583â†’            const colors = ['#e91e63', '#f44336', '#9c27b0', '#ff4081', '#ff6b9d', '#c2185b', '#ff80ab', '#f48fb1', '#ffd700', '#ff69b4'];\n   584â†’\n   585â†’            for (let i = 0; i < confettiCount; i++) {\n   586â†’                confetti.push({\n   587â†’                    x: Math.random() * canvas.width,\n   588â†’                    y: Math.random() * canvas.height - canvas.height,\n   589â†’                    size: Math.random() * 10 + 5,\n   590â†’                    color: colors[Math.floor(Math.random() * colors.length)],\n   591â†’                    speedY: Math.random() * 4 + 2,\n   592â†’                    speedX: Math.random() * 4 - 2,\n   593â†’                    rotation: Math.random() * 360,\n   594â†’                    rotationSpeed: Math.random() * 15 - 7.5,\n   595â†’                    shape: Math.random() > 0.5 ? 'heart' : 'rect'\n   596â†’                });\n   597â†’            }\n   598â†’\n   599â†’            function drawHeart(ctx, x, y, size, rotation) {\n   600â†’                ctx.save();\n   601â†’                ctx.translate(x, y);\n   602â†’                ctx.rotate(rotation * Math.PI / 180);\n   603â†’                ctx.beginPath();\n   604â†’                ctx.moveTo(0, size / 4);\n   605â†’                ctx.bezierCurveTo(size / 2, -size / 2, size, size / 4, 0, size);\n   606â†’                ctx.bezierCurveTo(-size, size / 4, -size / 2, -size / 2, 0, size / 4);\n   607â†’                ctx.fill();\n   608â†’                ctx.restore();\n   609â†’            }\n   610â†’\n   611â†’            function animate() {\n   612â†’                ctx.clearRect(0, 0, canvas.width, canvas.height);\n   613â†’\n   614â†’                let allGone = true;\n   615â†’\n   616â†’                confetti.forEach((c) => {\n   617â†’                    c.y += c.speedY;\n   618â†’                    c.x += c.speedX;\n   619â†’                    c.rotation += c.rotationSpeed;\n   620â†’\n   621â†’                    if (c.y < canvas.height + 50) {\n   622â†’                        allGone = false;\n   623â†’                    }\n   624â†’\n   625â†’                    ctx.fillStyle = c.color;\n   626â†’\n   627â†’                    if (c.shape === 'heart') {\n   628â†’                        drawHeart(ctx, c.x, c.y, c.size / 2, c.rotation);\n   629â†’                    } else {\n   630â†’                        ctx.save();\n   631â†’                        ctx.translate(c.x, c.y);\n   632â†’                        ctx.rotate(c.rotation * Math.PI / 180);\n   633â†’                        ctx.fillRect(-c.size / 2, -c.size / 2, c.size, c.size);\n   634â†’                        ctx.restore();\n   635â†’                    }\n   636â†’                });\n   637â†’\n   638â†’                if (!allGone) {\n   639â†’                    requestAnimationFrame(animate);\n   640â†’                }\n   641â†’            }\n   642â†’\n   643â†’            animate();\n   644â†’        }\n   645â†’\n   646â†’        // Handle window resize\n   647â†’        window.addEventListener('resize', () => {\n   648â†’            const canvas = document.getElementById('confettiCanvas');\n   649â†’            canvas.width = window.innerWidth;\n   650â†’            canvas.height = window.innerHeight;\n   651â†’        });\n   652â†’    </script>\n   653â†’</body>\n   654â†’</html>\n   655â†’",
