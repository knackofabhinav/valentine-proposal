</head>\n<body>\n    <div class=\"hearts-bg\" id=\"heartsBg\"></div>\n\n    <div class=\"container\" id=\"mainContainer\">\n        <!-- Generator Page -->\n        <div id=\"generator\" class=\"page active\">\n            <div class=\"main-emoji heartbeat\">ğŸ’•</div>\n            <h1>Create Your Valentine Proposal</h1>\n            <p class=\"subtitle\">Generate a special link for your loved one</p>\n\n            <div class=\"form-group\">\n                <label for=\"name\">Their Name</label>\n                <input type=\"text\" id=\"name\" placeholder=\"Enter their name\" autocomplete=\"off\">\n            </div>\n\n            <div class=\"form-group\">\n                <label for=\"relationship\">They are my</label>\n                <select id=\"relationship\">\n                    <option value=\"\">Select...</option>\n                    <option value=\"girlfriend\">Girlfriend</option>\n                    <option value=\"boyfriend\">Boyfriend</option>\n                    <option value=\"wife\">Wife</option>\n                    <option value=\"husband\">Husband</option>\n                    <option value=\"partner\">Partner</option>\n                    <option value=\"crush\">Crush</option>\n                    <option value=\"best friend\">Best Friend</option>\n                </select>\n            </div>\n\n            <button class=\"btn btn-primary\" onclick=\"generateLink()\">Generate Link</button>\n\n            <div id=\"linkResult\" class=\"link-result\">\n                <p>Share this link with them</p>\n                <div class=\"link-text\" id=\"generatedLink\"></div>\n                <button class=\"btn btn-secondary\" onclick=\"copyLink()\">Copy Link</button>\n            </div>\n        </div>\n\n        <!-- Proposal Page -->\n        <div id=\"proposal\" class=\"page\">\n            <div class=\"main-emoji heartbeat\">â¤ï¸</div>\n            <div class=\"greeting\">Hey <span id=\"displayName\"></span>!</div>\n            <div class=\"personalized-text\" id=\"personalizedText\"></div>\n            <div class=\"question\">Will you be my Valentine?</div>\n\n            <div class=\"button-container\" id=\"buttonContainer\">\n                <button class=\"btn btn-success yes-btn\" onclick=\"handleYes()\">Yes!</button>\n                <button class=\"btn no-btn\" id=\"noBtn\">No</button>\n            </div>\n        </div>\n\n        <!-- Success Page -->\n        <div id=\"success\" class=\"page\">\n            <div class=\"main-emoji\">ğŸ‰</div>\n            <div class=\"success-message\">Yay! You said YES!</div>\n            <p class=\"love-text\" id=\"loveText\">I'm so happy you'll be my Valentine!</p>\n            <div class=\"gif-container\">\n                <img src=\"https://media.giphy.com/media/v1.Y2lkPTc5MGI3NjExcGE4aHZ2ZjNvdGF0Ym16OWt6YnR3YnNhYWN6dTN5bTdvdGt6aXBtdCZlcD12MV9pbnRlcm5hbF9naWZfYnlfaWQmY3Q9Zw/L0O3TQpp0ljP2vwuIx/giphy.gif\" alt=\"Celebration\">\n            </div>\n            <div class=\"date-text\">February 14th - It's a date!</div>\n        </div>\n    </div>\n\n    <canvas id=\"confettiCanvas\"></canvas>\n\n    <script>\n        // Physics constants\n        const SPRING_STIFFNESS = 0.08;\n        const SPRING_DAMPING = 0.75;\n        const MOUSE_INFLUENCE = 0.02;\n\n        // State\n        let mouseX = window.innerWidth / 2;\n        let mouseY = window.innerHeight / 2;\n        let containerRotateX = 0;\n        let containerRotateY = 0;\n        let targetRotateX = 0;\n        let targetRotateY = 0;\n        let hearts = [];\n        let animationId;\n\n        // Initialize\n        document.addEventListener('DOMContentLoaded', init);\n\n        function init() {\n            createFloatingHearts();\n            setupMouseTracking();\n            setupNoButton();\n            startAnimation();\n            checkUrlParams();\n        }\n\n        // Floating hearts with physics\n        function createFloatingHearts() {\n            const container = document.getElementById('heartsBg');\n            const heartEmojis = ['â¤ï¸', 'ğŸ’•', 'ğŸ’–', 'ğŸ’—', 'ğŸ’“', 'ğŸ’˜', 'ğŸ’', 'ğŸ©·'];\n\n            for (let i = 0; i < 20; i++) {\n                const heart = document.createElement('div');\n                heart.className = 'floating-heart';\n                heart.textContent = heartEmojis[Math.floor(Math.random() * heartEmojis.length)];\n                heart.style.fontSize = (18 + Math.random() * 24) + 'px';\n                container.appendChild(heart);\n\n                hearts.push({\n                    element: heart,\n                    x: Math.random() * window.innerWidth,\n                    y: Math.random() * window.innerHeight + window.innerHeight,\n                    baseX: Math.random() * window.innerWidth,\n                    speedY: 0.3 + Math.random() * 0.5,\n                    amplitude: 20 + Math.random() * 40,\n                    frequency: 0.005 + Math.random() * 0.01,\n                    phase: Math.random() * Math.PI * 2,\n                    opacity: 0,\n                    targetOpacity: 0.4 + Math.random() * 0.3,\n                    parallaxFactor: 0.02 + Math.random() * 0.04\n                });\n            }\n        }\n\n        // Mouse tracking for parallax\n        function setupMouseTracking() {\n            const container = document.getElementById('mainContainer');\n\n            document.addEventListener('mousemove', (e) => {\n                mouseX = e.clientX;\n                mouseY = e.clientY;\n\n                // Calculate rotation based on mouse position\n                const centerX = window.innerWidth / 2;\n                const centerY = window.innerHeight / 2;\n                targetRotateY = (mouseX - centerX) / centerX * 8;\n                targetRotateX = -(mouseY - centerY) / centerY * 8;\n            });\n\n            // Touch support\n            document.addEventListener('touchmove', (e) => {\n                if (e.touches.length > 0) {\n                    mouseX = e.touches[0].clientX;\n                    mouseY = e.touches[0].clientY;\n                }\n            }, { passive: true });\n\n            // Reset on mouse leave\n            document.addEventListener('mouseleave', () => {\n                targetRotateX = 0;\n                targetRotateY = 0;\n            });\n        }\n\n        // No button behavior\n        function setupNoButton() {\n            const noBtn = document.getElementById('noBtn');\n            let isMoving = false;\n\n            const moveButton = () => {\n                if (isMoving) return;\n                isMoving = true;\n\n                const container = document.getElementById('buttonContainer');\n                const containerRect = container.getBoundingClientRect();\n                const btnRect = noBtn.getBoundingClientRect();\n\n                const maxX = containerRect.width - btnRect.width;\n                const maxY = containerRect.height - btnRect.height;\n\n                // Calculate new position away from current touch/mouse\n                let newX, newY;\n                let attempts = 0;\n\n                do {\n                    newX = Math.random() * maxX;\n                    newY = Math.random() * maxY;\n                    attempts++;\n                } while (attempts < 10 && Math.abs(newX - parseFloat(noBtn.style.left || 0)) < 50);\n\n                noBtn.style.left = newX + 'px';\n                noBtn.style.top = newY + 'px';\n\n                setTimeout(() => { isMoving = false; }, 500);\n            };\n\n            noBtn.addEventListener('mouseenter', moveButton);\n            noBtn.addEventListener('touchstart', (e) => {\n                e.preventDefault();\n                moveButton();\n            }, { passive: false });\n        }\n\n        // Main animation loop\n        function startAnimation() {\n            const container = document.getElementById('mainContainer');\n            let time = 0;\n\n            function animate() {\n                time += 1;\n\n                // Spring physics for container rotation\n                const dx = targetRotateX - containerRotateX;\n                const dy = targetRotateY - containerRotateY;\n                containerRotateX += dx * SPRING_STIFFNESS;\n                containerRotateY += dy * SPRING_STIFFNESS;\n                containerRotateX *= SPRING_DAMPING;\n                containerRotateY *= SPRING_DAMPING;\n\n                // Apply to container (subtle effect)\n                if (Math.abs(containerRotateX) > 0.01 || Math.abs(containerRotateY) > 0.01) {\n                    container.style.transform = `rotateX(${containerRotateX}deg) rotateY(${containerRotateY}deg)`;\n                }\n\n                // Animate hearts\n                hearts.forEach((heart, i) => {\n                    // Move up\n                    heart.y -= heart.speedY;\n\n                    // Horizontal wave motion\n                    const wave = Math.sin(time * heart.frequency + heart.phase) * heart.amplitude;\n                    heart.x = heart.baseX + wave;\n\n                    // Parallax based on mouse\n                    const parallaxX = (mouseX - window.innerWidth / 2) * heart.parallaxFactor;\n                    const parallaxY = (mouseY - window.innerHeight / 2) * heart.parallaxFactor;\n\n                    // Fade in/out\n                    if (heart.y < window.innerHeight && heart.y > -50) {\n                        heart.opacity += (heart.targetOpacity - heart.opacity) * 0.05;\n                    } else if (heart.y <= -50) {\n                        heart.opacity *= 0.95;\n                    }\n\n                    // Reset when off screen\n                    if (heart.y < -100) {\n                        heart.y = window.innerHeight + 50;\n                        heart.baseX = Math.random() * window.innerWidth;\n                        heart.opacity = 0;\n                    }\n\n                    // Apply transforms\n                    heart.element.style.transform = `translate(${heart.x + parallaxX}px, ${heart.y + parallaxY}px)`;\n                    heart.element.style.opacity = heart.opacity;\n                });\n\n                animationId = requestAnimationFrame(animate);\n            }\n\n            animate();\n        }\n\n        // URL params check\n        function checkUrlParams() {\n            const params = new URLSearchParams(window.location.search);\n            const name = params.get('name');\n            const rel = params.get('rel');\n\n            if (name && rel) {\n                showPage('proposal');\n                const decodedName = decodeURIComponent(name);\n                document.getElementById('displayName').textContent = decodedName;\n                document.getElementById('personalizedText').textContent = getPersonalizedMessage(rel, decodedName);\n            }\n        }\n\n        function getPersonalizedMessage(relationship, name) {\n            const messages = {\n                'girlfriend': `You mean the world to me, ${name}.`,\n                'boyfriend': `You make every day better, ${name}.`,\n                'wife': `Every day with you is a blessing, ${name}.`,\n                'husband': `You're my forever person, ${name}.`,\n                'partner': `Life is beautiful with you, ${name}.`,\n                'crush': `I've been wanting to ask you something...`,\n                'best friend': `You're my favorite person, ${name}.`\n            };\n            return messages[relationship] || `You're special to me, ${name}.`;\n        }\n\n        // Page transitions\n        function showPage(pageId) {\n            document.querySelectorAll('.page').forEach(page => {\n                page.classList.remove('active');\n            });\n            setTimeout(() => {\n                document.getElementById(pageId).classList.add('active');\n            }, 50);\n        }\n\n        // Generate link\n        function generateLink() {\n            const name = document.getElementById('name').value.trim();\n            const relationship = document.getElementById('relationship').value;\n\n            if (!name || !relationship) {\n                // Subtle shake animation\n                const container = document.getElementById('mainContainer');\n                container.style.animation = 'none';\n                container.offsetHeight; // Trigger reflow\n                container.style.animation = 'shake 0.5s ease';\n                return;\n            }\n\n            const baseUrl = window.location.href.split('?')[0];\n            const link = `${baseUrl}?name=${encodeURIComponent(name)}&rel=${encodeURIComponent(relationship)}`;\n\n            document.getElementById('generatedLink').textContent = link;\n            document.getElementById('linkResult').classList.add('show');\n        }\n\n        // Copy link\n        function copyLink() {\n            const linkText = document.getElementById('generatedLink').textContent;\n            navigator.clipboard.writeText(linkText).then(() => {\n                const btn = document.querySelector('.link-result .btn');\n                const originalText = btn.textContent;\n                btn.textContent = 'Copied!';\n                btn.style.background = 'linear-gradient(135deg, #4caf50 0%, #8bc34a 100%)';\n                setTimeout(() => {\n                    btn.textContent = originalText;\n                    btn.style.background = '';\n                }, 2000);\n            });\n        }\n\n        // Handle Yes click\n        function handleYes() {\n            showPage('success');\n\n            const params = new URLSearchParams(window.location.search);\n            const name = params.get('name');\n            if (name) {\n                document.getElementById('loveText').textContent = `Thank you, ${decodeURIComponent(name)}! You've made me so happy!`;\n            }\n\n            createConfetti();\n        }\n\n        // Premium confetti with physics\n        function createConfetti() {\n            const canvas = document.getElementById('confettiCanvas');\n            const ctx = canvas.getContext('2d');\n\n            canvas.width = window.innerWidth;\n            canvas.height = window.innerHeight;\n\n            const particles = [];\n            const colors = ['#e91e63', '#ff4081', '#ff6b9d', '#f48fb1', '#ce93d8', '#ba68c8', '#ffd700', '#ff69b4', '#ff1744', '#f50057'];\n            const gravity = 0.12;\n            const drag = 0.985;\n            const terminalVelocity = 8;\n\n            // Create particles\n            for (let i = 0; i < 250; i++) {\n                const angle = Math.random() * Math.PI * 2;\n                const velocity = 8 + Math.random() * 12;\n\n                particles.push({\n                    x: canvas.width / 2,\n                    y: canvas.height / 2,\n                    vx: Math.cos(angle) * velocity,\n                    vy: Math.sin(angle) * velocity - 8,\n                    size: 6 + Math.random() * 8,\n                    color: colors[Math.floor(Math.random() * colors.length)],\n                    rotation: Math.random() * 360,\n                    rotationSpeed: (Math.random() - 0.5) * 12,\n                    shape: Math.random() > 0.4 ? 'rect' : 'heart',\n                    opacity: 1\n                });\n            }\n\n            function drawHeart(x, y, size, rotation) {\n                ctx.save();\n                ctx.translate(x, y);\n                ctx.rotate(rotation * Math.PI / 180);\n                ctx.beginPath();\n                ctx.moveTo(0, size * 0.3);\n                ctx.bezierCurveTo(size * 0.5, -size * 0.3, size, size * 0.3, 0, size);\n                ctx.bezierCurveTo(-size, size * 0.3, -size * 0.5, -size * 0.3, 0, size * 0.3);\n                ctx.fill();\n                ctx.restore();\n            }\n\n            function animate() {\n                ctx.clearRect(0, 0, canvas.width, canvas.height);\n\n                let activeCount = 0;\n\n                particles.forEach(p => {\n                    // Physics\n                    p.vy += gravity;\n                    p.vy = Math.min(p.vy, terminalVelocity);\n                    p.vx *= drag;\n                    p.vy *= drag;\n\n                    p.x += p.vx;\n                    p.y += p.vy;\n                    p.rotation += p.rotationSpeed;\n\n                    // Fade out when below screen\n                    if (p.y > canvas.height - 100) {\n                        p.opacity *= 0.96;\n                    }\n\n                    if (p.opacity > 0.01 && p.y < canvas.height + 50) {\n                        activeCount++;\n\n                        ctx.globalAlpha = p.opacity;\n                        ctx.fillStyle = p.color;\n\n                        if (p.shape === 'heart') {\n                            drawHeart(p.x, p.y, p.size * 0.5, p.rotation);\n                        } else {\n                            ctx.save();\n                            ctx.translate(p.x, p.y);\n                            ctx.rotate(p.rotation * Math.PI / 180);\n                            ctx.fillRect(-p.size / 2, -p.size / 2, p.size, p.size * 0.6);\n                            ctx.restore();\n                        }\n                    }\n                });\n\n                ctx.globalAlpha = 1;\n\n                if (activeCount > 0) {\n                    requestAnimationFrame(animate);\n                }\n            }\n\n            animate();\n        }\n\n        // Handle resize\n        window.addEventListener('resize', () => {\n            const canvas = document.getElementById('confettiCanvas');\n            canvas.width = window.innerWidth;\n            canvas.height = window.innerHeight;\n        });\n\n        // Add shake animation\n        const style = document.createElement('style');\n        style.textContent = `\n            @keyframes shake {\n                0%, 100% { transform: translateX(0) rotateX(${containerRotateX}deg) rotateY(${containerRotateY}deg); }\n                20% { transform: translateX(-8px) rotateX(${containerRotateX}deg) rotateY(${containerRotateY}deg); }\n                40% { transform: translateX(8px) rotateX(${containerRotateX}deg) rotateY(${containerRotateY}deg); }\n                60% { transform: translateX(-8px) rotateX(${containerRotateX}deg) rotateY(${containerRotateY}deg); }\n                80% { transform: translateX(8px) rotateX(${containerRotateX}deg) rotateY(${containerRotateY}deg); }\n            }\n        `;\n        document.head.appendChild(style);\n    </script>\n</body>\n</html>\n",
