       const params = new URLSearchParams(window.location.search);\n            return {\n                name: params.get('name'),\n                rel: params.get('rel')\n            };\n        }\n\n        // Get personalized message based on relationship\n        function getPersonalizedMessage(relationship, name) {\n            const messages = {\n                'girlfriend': `You mean the world to me, ${name}.`,\n                'boyfriend': `You make every day better, ${name}.`,\n                'wife': `Every day with you is a blessing, ${name}.`,\n                'husband': `You're my forever person, ${name}.`,\n                'partner': `Life is beautiful with you, ${name}.`,\n                'crush': `I've been wanting to ask you something...`,\n                'best friend': `You're my favorite person, ${name}.`\n            };\n            return messages[relationship] || `You're special to me, ${name}.`;\n        }\n\n        // Initialize page\n        window.onload = function() {\n            createFloatingHearts();\n            const params = getUrlParams();\n\n            if (params.name && params.rel) {\n                // Show proposal page\n                document.getElementById('generator').style.display = 'none';\n                document.getElementById('proposal').classList.add('show');\n\n                const decodedName = decodeURIComponent(params.name);\n                document.getElementById('displayName').textContent = decodedName;\n                document.getElementById('personalizedText').textContent = getPersonalizedMessage(params.rel, decodedName);\n            }\n        };\n\n        // Generate link\n        function generateLink() {\n            const name = document.getElementById('name').value.trim();\n            const relationship = document.getElementById('relationship').value;\n\n            if (!name || !relationship) {\n                alert('Please fill in all fields!');\n                return;\n            }\n\n            const baseUrl = window.location.href.split('?')[0];\n            const link = `${baseUrl}?name=${encodeURIComponent(name)}&rel=${encodeURIComponent(relationship)}`;\n\n            document.getElementById('generatedLink').textContent = link;\n            document.getElementById('linkResult').classList.add('show');\n        }\n\n        // Copy link\n        function copyLink() {\n            const linkText = document.getElementById('generatedLink').textContent;\n            navigator.clipboard.writeText(linkText).then(() => {\n                const btn = document.querySelector('.copy-btn');\n                btn.textContent = 'Copied!';\n                setTimeout(() => {\n                    btn.textContent = 'Copy Link';\n                }, 2000);\n            });\n        }\n\n        // Move No button randomly\n        let noMoveCount = 0;\n        function moveNoButton() {\n            noMoveCount++;\n            const noBtn = document.getElementById('noBtn');\n            const container = document.getElementById('buttonContainer');\n\n            const containerRect = container.getBoundingClientRect();\n            const btnRect = noBtn.getBoundingClientRect();\n\n            const maxX = containerRect.width - btnRect.width;\n            const maxY = containerRect.height - btnRect.height;\n\n            const randomX = Math.random() * maxX;\n            const randomY = Math.random() * maxY;\n\n            noBtn.style.left = randomX + 'px';\n            noBtn.style.top = randomY + 'px';\n\n            // Make button smaller each time\n            if (noMoveCount > 3) {\n                noBtn.style.transform = `scale(${Math.max(0.5, 1 - noMoveCount * 0.1)})`;\n            }\n\n            // Change text after a few attempts\n            if (noMoveCount === 3) {\n                noBtn.textContent = 'Are you sure?';\n            } else if (noMoveCount === 5) {\n                noBtn.textContent = 'Really?';\n            } else if (noMoveCount >= 7) {\n                noBtn.textContent = 'Just say yes!';\n            }\n        }\n\n        // Handle Yes click\n        function handleYes() {\n            document.getElementById('proposal').classList.remove('show');\n            document.getElementById('success').classList.add('show');\n            createConfetti();\n\n            // Update love text with name\n            const params = getUrlParams();\n            if (params.name) {\n                const name = decodeURIComponent(params.name);\n                document.getElementById('loveText').textContent = `Thank you, ${name}! You've made me so happy!`;\n            }\n        }\n\n        // Confetti effect\n        function createConfetti() {\n            const canvas = document.getElementById('confettiCanvas');\n            const ctx = canvas.getContext('2d');\n\n            canvas.width = window.innerWidth;\n            canvas.height = window.innerHeight;\n\n            const confettiCount = 200;\n            const confetti = [];\n\n            const colors = ['#e91e63', '#f44336', '#9c27b0', '#ff4081', '#ff6b9d', '#c2185b', '#ff80ab', '#f48fb1', '#ffd700', '#ff69b4'];\n\n            for (let i = 0; i < confettiCount; i++) {\n                confetti.push({\n                    x: Math.random() * canvas.width,\n                    y: Math.random() * canvas.height - canvas.height,\n                    size: Math.random() * 10 + 5,\n                    color: colors[Math.floor(Math.random() * colors.length)],\n                    speedY: Math.random() * 4 + 2,\n                    speedX: Math.random() * 4 - 2,\n                    rotation: Math.random() * 360,\n                    rotationSpeed: Math.random() * 15 - 7.5,\n                    shape: Math.random() > 0.5 ? 'heart' : 'rect'\n                });\n            }\n\n            function drawHeart(ctx, x, y, size, rotation) {\n                ctx.save();\n                ctx.translate(x, y);\n                ctx.rotate(rotation * Math.PI / 180);\n                ctx.beginPath();\n                ctx.moveTo(0, size / 4);\n                ctx.bezierCurveTo(size / 2, -size / 2, size, size / 4, 0, size);\n                ctx.bezierCurveTo(-size, size / 4, -size / 2, -size / 2, 0, size / 4);\n                ctx.fill();\n                ctx.restore();\n            }\n\n            function animate() {\n                ctx.clearRect(0, 0, canvas.width, canvas.height);\n\n                let allGone = true;\n\n                confetti.forEach((c) => {\n                    c.y += c.speedY;\n                    c.x += c.speedX;\n                    c.rotation += c.rotationSpeed;\n\n                    if (c.y < canvas.height + 50) {\n                        allGone = false;\n                    }\n\n                    ctx.fillStyle = c.color;\n\n                    if (c.shape === 'heart') {\n                        drawHeart(ctx, c.x, c.y, c.size / 2, c.rotation);\n                    } else {\n                        ctx.save();\n                        ctx.translate(c.x, c.y);\n                        ctx.rotate(c.rotation * Math.PI / 180);\n                        ctx.fillRect(-c.size / 2, -c.size / 2, c.size, c.size);\n                        ctx.restore();\n                    }\n                });\n\n                if (!allGone) {\n                    requestAnimationFrame(animate);\n                }\n            }\n\n            animate();\n        }\n\n        // Handle window resize\n        window.addEventListener('resize', () => {\n            const canvas = document.getElementById('confettiCanvas');\n            canvas.width = window.innerWidth;\n            canvas.height = window.innerHeight;\n        });\n    </script>\n</body>\n</html>\n",
